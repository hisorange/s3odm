{
  "name": "@hisorange/s3odm",
  "version": "1.1.1",
  "description": "Lightweight S3 Object Data Model",
  "keywords": [
    "s3",
    "odm",
    "r2",
    "light weight",
    "orm"
  ],
  "exports": {
    ".": "./lib/s3odm.js"
  },
  "main": "./lib/s3odm.js",
  "types": "./lib/s3odm.d.ts",
  "repository": "https://github.com/hisorange/s3odm",
  "author": "Varga Zsolt <hello@hisorange.me>",
  "license": "GPL-3.0",
  "private": false,
  "type": "module",
  "files": [
    "lib/"
  ],
  "entrypoints": {
    "s3odm": "./lib/s3odm.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.16.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:prod": "tsc -p tsconfig.prod.json",
    "test": "jest --coverage=0 --verbose --silent=0",
    "test:ci": "jest --runInBand",
    "dev": "node --experimental-specifier-resolution=node --no-warnings --loader ts-node/esm src/dev.ts"
  },
  "dependencies": {
    "html-rewriter-wasm": "^0.4.1",
    "node-fetch": "^3.2.3"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.33",
    "jest": "^28.1.0",
    "nyc": "^15.1.0",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
}